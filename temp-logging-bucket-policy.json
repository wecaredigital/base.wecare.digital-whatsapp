{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowBedrockLogging",
            "Effect": "Allow",
            "Principal": {
                "Service": "bedrock.amazonaws.com"
            },
            "Action": [
                "s3:PutObject"
            ],
            "Resource": "arn:aws:s3:::base-wecare-digital-whatsapp-bedrock-logs/bedrock-logs/*",
            "Condition": {
                "StringEquals": {
                    "aws:SourceAccount": "010526260063"
                },
                "ArnLike": {
                    "aws:SourceArn": "arn:aws:bedrock:ap-south-1:010526260063:*"
                }
            }
        }
    ]
}